	<th:block th:insert="~{/include/footer :: mtFooter}"></th:block>
	
		<!-- 이 div 안에 insert 하기 -->
	<div th:insert="~{/include/footer :: myFooter}" style="border:1px dooted blue;"></div>
	
		<!--  이 div를 replace(대체) 하기 -->
	<div th:replace="~{/include/footer :: myFooter}" style="border:1px dooted blue;"></div>
	
	<div th:replace="~{/include/footer :: yourFooter('hi footer')}" ></div>
	<div th:replace="~{/include/footer :: yourFooter('bye footer')}" ></div>
	
	
</body>
